										div.x_content
											div(class='bs-example' data-example-id='simple-jumbotron')
												div.jumbotron
													div.minifoto.col-md-12
														img(src='/images/equipo/pablo.png')
													p @peleaApunietazo
													p A veces me las doy de programador
